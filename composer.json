{
    "name": "knplabs/rad-fixtures-load",
    "description": "A command to load them all",
    "license": "MIT",
    "keywords": ["Symfony2", "alice", "fixtures", "load", "command"],
    "authors": [
        {
            "name": "KNPLabs Team",
            "email": "hello@knplabs.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "doctrine/common": "~2.0",
        "doctrine/orm": "~2.0",
        "nelmio/alice": "~1.5||~2.0",
        "symfony/dependency-injection": "^2.8|^3.0|^4.0",
        "symfony/finder": "^2.8|^3.0|^4.0",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/http-kernel": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "pedrotroller/php-cs-custom-fixer": "^2.15",
        "phpspec/phpspec": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Knp\\Rad\\FixturesLoad\\": "src/Knp/Rad/FixturesLoad/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
